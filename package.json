    "codacy-coverage": "^3.4.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.0",
    "css-loader": "3.2.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron": "^7.0.0",
    "electron-builder": "^22.1.0",
    "electron-download": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-react-app": "^5.0.0",
    "eslint-loader": "3.0.1",
    "eslint-plugin-flowtype": "4.3.0",
