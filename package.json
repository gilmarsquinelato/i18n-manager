    "redux-saga": "^1.0.4",
    "typescript": "3.5.2",
    "uuid": "^3.3.2"
  },
  "build": {
    "productName": "i18n Manager",
    "appId": "com.i18n-manager.app",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "icons/**/*",
      "package.json"
    ],
    "icon": "./icons/icon.png",
    "mac": {
      "publish": [
        "github"
      ],
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ],
      "extendInfo": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Folder",
            "CFBundleTypeOSTypes": [
              "fold"
            ],
            "CFBundleTypeRole": "Editor",
            "LSIsAppleDefaultForType": true
          }
        ]
      }
    },
    "linux": {
      "publish": [
        "github"
      ],
      "category": "Development"
    },
    "win": {
      "publish": [
        "github"
      ]
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
