    "@types/redux-actions": "^2.6.1",
    "@types/redux-saga": "^0.10.5",
    "@types/semver": "^6.0.1",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "2.6.1",
